        for(int i = 0; i < N1; i++) scanf("%d", &arr1[i]);

        scanf("%d", &N2);
        int arr2[N2];
        for(int i = 0; i < N2; i++) scanf("%d", &arr2[i]);

        int i = 0, j = 0;
        while(i < N1 && j < N2) {
            if(arr1[i] < arr2[j]) i++;
            else if(arr1[i] > arr2[j]) j++;
            else {
                printf("%d ", arr1[i]);
                i++; j++;
            }
        }
        printf("\n");

EXP 5
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i=0; i<n; i++) scanf("%d", &arr[i]);
    int k;
    scanf("%d", &k);

    int i = 0, j = 1;
    while(i < n && j < n) {
        if(i == j) { j++; continue; }
        int diff = arr[j] - arr[i];
        if(diff == k) {
            printf("1");
            return 0;
        } else if(diff < k) {
            j++;
        } else {
            i++;
        }
    }
    printf("0");
    return 0;
}

EXP 6
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i=0; i<n; i++) scanf("%d", &arr[i]);
    int k;
    scanf("%d", &k);

    int i = 0, j = 1;
    while(i < n && j < n) {
        if(i == j) { j++; continue; }
        int diff = arr[j] - arr[i];
        if(diff == k) {
            printf("1");
            return 0;
        } else if(diff < k) {
            j++;
        } else {
            i++;
        }
    }
    printf("0");
    return 0;
}
