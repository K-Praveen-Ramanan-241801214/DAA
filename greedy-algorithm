EXP 1
#include <stdio.h>

int main() {
    int V;
    scanf("%d", &V);

    int denominations[] = {1000, 500, 100, 50, 20, 10, 5, 2, 1};
    int n = sizeof(denominations) / sizeof(denominations[0]);

    int count = 0;
    for (int i = 0; i < n; i++) {
        if (V >= denominations[i]) {
            count += V / denominations[i];  
            V = V % denominations[i];       
        }
    }

    printf("%d\n", count);

    return 0;
}

EXP 2
#include <stdio.h>
#include <stdlib.h>

int cmpfunc(const void *a, const void *b) {
    return (*(int*)a - *(int*)b);
}

int main() {
    int n, m;
    scanf("%d", &n);   
    
    int g[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &g[i]);
    
    scanf("%d", &m);   
    
    int s[m];
    for (int i = 0; i < m; i++)
        scanf("%d", &s[i]);

    qsort(g, n, sizeof(int), cmpfunc);
    qsort(s, m, sizeof(int), cmpfunc);

    int i = 0, j = 0, content = 0;

    while (i < n && j < m) {
        if (s[j] >= g[i]) {  
            content++;  
            i++;        
        }
        j++;  
    }

    printf("%d\n", content);
    return 0;
}

EXP 3
#include<stdio.h>
int main(){
    int b;
    scanf("%d",&b);
    int arr[b];
    for (int i=0;i<b;i++){
        scanf("%d",&arr[i]);
    }
    int i=0,d=0;
    while(i<(b-1)){
        if(arr[i]<arr[i+1]){
            d=arr[i+1];
            arr[i+1]=arr[i];
            arr[i]=d;
            i=0;
            continue;
        }
        i++;
    }
    int c=0;
    int s=0;
    for(int j=0;j<b;j++){
        s=1;
        for(int k=0;k<j;k++){
            s=s*b;
        }
        ;;
        c=c+(s*arr[j]);
    }
    printf("%d",c);
}

EXP 4
#include <stdio.h>
#include <stdlib.h>

int cmpfunc(const void *a, const void *b) {
    return (*(int*)a - *(int*)b); 
}

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++) scanf("%d", &arr[i]);

    qsort(arr, n, sizeof(int), cmpfunc);

    long long sum = 0;
    for (int i = 0; i < n; i++) {
        sum += (long long)arr[i] * i;
    }

    printf("%lld\n", sum);
    return 0;
}

EXP 5
#include <stdio.h>
#include <stdlib.h>

int cmp_asc (const void *a, const void *b)  { 
    long long x = *(const long long*)a, y = *(const long long*)b;
    return (x > y) - (x < y);
}
int cmp_desc(const void *a, const void *b)  { 
    long long x = *(const long long*)a, y = *(const long long*)b;
    return (y > x) - (y < x);
}

int main() {
    int N;
    if (scanf("%d", &N) != 1) return 0;

    long long *A = (long long*)malloc(sizeof(long long)*N);
    long long *B = (long long*)malloc(sizeof(long long)*N);

    for (int i = 0; i < N; i++) scanf("%lld", &A[i]);
    for (int i = 0; i < N; i++) scanf("%lld", &B[i]);

    qsort(A, N, sizeof(long long), cmp_asc);
    qsort(B, N, sizeof(long long), cmp_desc);

    long long ans = 0;
    for (int i = 0; i < N; i++) ans += A[i] * B[i];

    printf("%lld\n", ans);

    free(A); free(B);
    return 0;
}
